{
  "name": "wewatch",
  "version": "0.1.0",
  "description": "weWatch is a social app for sharing media streaming",
  "main": "server.js",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.6.0",
    "compression": "^1.7.4",
    "concurrently": "^6.0.2",
    "cookie-parser": "^1.4.5",
    "crypto-js": "^4.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-jwt": "^6.0.0",
    "express-session": "^1.17.1",
    "helmet": "^4.6.0",
    "is-empty": "^1.2.0",
    "jquery": "^3.6.0",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^5.12.5",
    "node-fetch": "^2.6.1",
    "particles-bg": "^2.5.5",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "react-filter-search": "^1.0.11",
    "react-search-field": "^1.2.1",
    "validator": "^13.6.0",
    "web-vitals": "^1.1.1",
    "xml-js": "^1.6.11"
  },
  "scripts": {
    "client-install": "npm install --prefix client",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "server": "nodemon server.js",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
}
